{
  "name": "devgiants/phar-application",
  "type": "project",
  "description": "Boilerplate for fully maintenable PHP CLI applications packaged in PHAR",
  "keywords": ["docker", "php", "cli", "phar", "console"],
  "homepage": "https://github.com/devgiants/phar-application",
  "license": "MIT",
  "require": {
    "symfony/console": "^3.2",
    "symfony/yaml": "^3.2",
    "symfony/config": "^3.2",
    "pimple/pimple": "~3.0",
    "herrera-io/phar-update": "~1.0",
    "monolog/monolog": "^1.23",
    "kriswallsmith/buzz": "^0.15.2",
    "symfony/finder": "^3.4",
    "symfony/dotenv": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests"
    }
  }
}
